<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/dogFragment">

    <fragment
        android:id="@+id/dogFragment"
        android:name="com.example.myapplication.ui.dog.dogview.DogFragment"
        android:label="fragment_dog"
        tools:layout="@layout/fragment_dog" >
        <action
            android:id="@+id/action_dogFragment_to_dogInfoFRagment"
            app:destination="@id/dogInfoFRagment" />
        <action
            android:id="@+id/action_dogFragment_to_newDogFragment"
            app:destination="@id/newDogFragment" />
    </fragment>
    <fragment
        android:id="@+id/dogInfoFRagment"
        android:name="com.example.myapplication.ui.dog.DogInfoFRagment"
        android:label="fragment_dog_info_f_ragment"
        tools:layout="@layout/fragment_dog_info_f_ragment" />
    <fragment
        android:id="@+id/newDogFragment"
        android:name="com.example.myapplication.ui.dog.newdog.NewDogFragment"
        android:label="fragment_new_dog"
        tools:layout="@layout/fragment_new_dog" />
</navigation>